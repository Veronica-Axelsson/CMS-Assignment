@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
@{
	Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>@Model.Value("PageTitle") | Crito</title>
    <meta name="description" content="Model.Value("PageDescription")">
    <meta name="keywords" content="@Model.Value("pageKeywords")">
    <meta name="author" content="Name">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="https://kit.fontawesome.com/41c8b474e2.js" crossorigin="anonymous"></script>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-4bw+/aepP/YC94hEpVNVgiZdgIC5+VKNBQNGCHeKRQN+PtmoHDEXuppvnDJzQIu9" crossorigin="anonymous">
    <link href="/css/style.css" rel="stylesheet">
    <script defer src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-HwwvtgBNo3bZJJLYd8oVXjrBZt8cqVSpeBNS5n7C8IVInixGAoxmnlMuBnhbgrkm" crossorigin="anonymous"></script>
    <script defer src="js/script.js"></script>
</head>
<body>
    
    <div class="wrapper">
        
        <header>
        @*  <img class="background-lines" src="~media/images/background.svg" alt="" />*@
          @*  <img class="background-lines" src="~/images/static/background-effects/background-lines.svg" alt="" />*@

            @await Html.PartialAsync("_NavSection")
            @await Html.PartialAsync("_ShowcaseSection")

        </header>

        <main>
            @RenderBody()
            
            @await Html.PartialAsync("_AboutCompany")
            @await Html.PartialAsync("_OurServices")
            @await Html.PartialAsync("_ChooseUs")
            @await Html.PartialAsync("_ProjectCaseStudies")
            @await Html.PartialAsync("_MeetTeam")
            @await Html.PartialAsync("_Testimonial")
            @await Html.PartialAsync("_ArticleNews")
            @await Html.PartialAsync("_Newsletter")
        </main>

        @await Html.PartialAsync("_FooterSection")

    </div>
</body>
</html>
